{
  "name": "interactive-cv",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Interactive web CV automatically generated from LaTeX resume",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
