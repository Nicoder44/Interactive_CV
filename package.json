    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^4.0.15",
    "gh-pages": "^6.1.1",
    "jsdom": "^27.2.0",
    "vite": "^5.4.11",
    "vitest": "^4.0.15"
  }
}
